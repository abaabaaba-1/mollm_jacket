{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: New SACS Optimization",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "sacs/config.yaml"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python: Resume SACS Optimization",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "args": [
        "sacs/config.yaml",
        "--resume"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}
